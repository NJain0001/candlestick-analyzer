{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [{
      "Name": "Async",
      "Args": {
        "configure": [{
          "Name": "File",
          "Args": {
            "path": "C:\\candlestick_logs\\logs.txt",
            "rollingInterval": "Day",
            "retainedFileCountLimit": null,
            "formatter": "Serilog.Formatting.Json.JsonFormatter"
          }
        }]
      }
    }]
  }
}
